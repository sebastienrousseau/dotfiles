#compdef dot

_dot() {
  local -a commands
  commands=(
    apply
    sync
    update
    diff
    remove
    drift
    history
    doctor
    keys
    tools
    sandbox
    benchmark
    tune
    secrets
    secrets-init
    secrets-create
    ssh-key
    firewall
    telemetry
    dns-doh
    encrypt-check
    backup
    lock-screen
    usb-safety
    fonts
    wallpaper
    theme
    upgrade
    edit
    docs
    new
    log-rotate
    help
  )
  _describe 'command' commands
}

_dot "$@"
