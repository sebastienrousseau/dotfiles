-w "\n"                 # Automatically add newline at end of curl response body.
connect-timeout = 60    # Wait 60 seconds before timing out.
location                # Follow redirects (default).
progress-bar            # Display progress as a simple progress bar.
referer = ";auto"       # When following a redirect, automatically set the previous URL as referer.
show-error              # Show error messages.
user-agent = "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/106.0.0.0 Safari/537.36 Edg/105.0.1343.53" # Send a fake UA string for the HTTP servers that sniff it
verbose                 # Show verbose information.
